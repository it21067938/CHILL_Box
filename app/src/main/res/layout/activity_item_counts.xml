<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".count.ItemCounts">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="889dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <TextView
            android:id="@+id/countlyc"
            android:layout_width="275dp"
            android:layout_height="50dp"
            android:layout_marginLeft="10dp"

            android:layout_marginTop="75dp"
            android:layout_marginRight="25dp"
            android:drawableRight="@drawable/ic_lyrics_24"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/countGcode"
            android:layout_width="282dp"
            android:layout_height="50dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="75dp"
            android:layout_marginRight="25dp"
            android:drawableRight="@drawable/ic_music_note_24"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/countVid"
            android:layout_width="294dp"
            android:layout_height="50dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="75dp"
            android:layout_marginRight="25dp"
            android:drawableRight="@drawable/ic_baseline_videocam_24"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="16dp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>